{
  "name": "Slack RTL Support",
  "version": "1.0",
  "description": "Extension to support RTL languages in slack web app.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://*.slack.com/*"],
      "js": ["main.js"]
    }
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  }
}
